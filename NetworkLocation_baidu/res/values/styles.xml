<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Transparent">
        <item name="android:windowBackground">@color/transparent</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowAnimationStyle">@android:style/Animation.Translucent</item>
    </style>
    <style name="CustomDialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
    </style>
    <style name="CustomDialog.TitleHuawei" parent="@style/CustomDialog">
        <item name="android:textSize">22.0sp</item>
        <item name="android:textColor">#ff23a7d9</item>
    </style>
    <style name="CustomDialog.MessageHuawei" parent="@style/CustomDialog">
        <item name="android:textSize">18.0sp</item>
        <item name="android:textColor">#ff000000</item>
    </style>
    <style name="CustomDialog.ButtonHuawei" parent="@style/CustomDialog">
        <item name="android:textSize">18.0sp</item>
        <item name="android:textColor">#ff000000</item>
    </style>
</resources>
